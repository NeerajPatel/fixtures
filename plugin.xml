    <resource>academic.School</resource>
    <resource>academic.Staff</resource>
    <resource>academic.Term</resource>
    <resource>AutowireMe</resource>
    <resource>Child</resource>
    <resource>com.book.Author</resource>
    <resource>com.book.Book</resource>
    <resource>Comment</resource>
    <resource>Grandparent</resource>
